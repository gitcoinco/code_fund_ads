<div id="cf" data-template="centered" data-theme="{{theme}}">
  <div class="cf-wrapper">
    <div class="cf-header">Proudly sponsored by</div>
    <a href="{{urls.campaign}}" class="cf-text" target="_blank" rel="sponsored noopener">
      <strong>{{creative.headline}}</strong>
      <span>{{creative.body}}</span>
    </a>
    <a href="{{urls.poweredBy}}" class="cf-powered-by" target="_blank" rel="sponsored noopener">
      <em>ethical</em> ad by CodeFund
      <img src="{{urls.instantImpression}}" data-src="{{urls.impression}}" data-behavior="trackImpression" class="cf-impression" alt="">
    </a>
  </div>
</div>
